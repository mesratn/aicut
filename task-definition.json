{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "aiserver",
            "name": "aiserver",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        },
        {
            "essential": true,
            "image": "client",
            "name": "client",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "root"
                }
            ],
            "essential": true,
            "image": "mysql:latest",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/mysql",
                    "sourceVolume": "Db-Data"
                }
            ],
            "name": "db"
        },
        {
            "essential": true,
            "image": "server",
            "name": "server",
            "portMappings": [
                {
                    "containerPort": 8082,
                    "hostPort": 8082
                },
                {
                    "containerPort": 9229,
                    "hostPort": 9229
                },
                {
                    "containerPort": 9230,
                    "hostPort": 9230
                }
            ]
        }
    ],
    "volumes": [
        {
            "host": {
                "sourcePath": "db-data"
            },
            "name": "Db-Data"
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "inferenceAccelerators": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::357890162125:role/ecsTaskExecutionRole",
    "family": "aicut-task-definition",
    "taskRoleArn": "",
    "placementConstraints": []
}
